    text = text.replace(new RegExp('[]', 'gi'), 'i');
    text = text.replace(new RegExp('[]', 'gi'), 'o');
    text = text.replace(new RegExp('[]', 'gi'), 'u');
    text = text.replace(new RegExp('[]', 'gi'), 'c');
    return text;
  },
