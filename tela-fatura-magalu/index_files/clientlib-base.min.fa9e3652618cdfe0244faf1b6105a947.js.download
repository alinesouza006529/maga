function srcResOpenVideo(codVideo, videoSpan) {
  var largura = window.innerWidth;
  var altura = window.innerHeight - 50;
  var videoSize = defineVideoSize();
  videoAtual = videoSpan;
  player = new YT.Player('framevideo', {
    videoId: codVideo,
