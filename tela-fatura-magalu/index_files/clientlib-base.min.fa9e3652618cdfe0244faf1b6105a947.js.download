function defineVideoSize() {
  screenWidth = $(window).width();
  screenHeight = $(window).height();
  var videoSize;

  if (screenWidth <= 840 && screenWidth > 655) {
    videoSize = [640, 410];
  } else if (screenWidth <= 655 && screenWidth > 380) {
    videoSize = [350, 200];
  } else {
    videoSize = [818, 460];
  }

  return videoSize;
}
