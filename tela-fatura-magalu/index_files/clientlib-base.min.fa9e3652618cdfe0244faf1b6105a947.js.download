function evtVideoStatus() {
  var infoEvent = $.parseJSON(event.data).info;

  if (infoEvent == YT.PlayerState.ENDED) {
    player.destroy();
    $("#lightboxvideo").css("display", "none");
    $("body").css("overflow", "");
  }
}

