function setDynamicSectionId() {
  $("section:not(.keepId)").each(function (index) {
    $(this).attr("id", "section" + (index + 1));

    if ($(this).parent('div').hasClass('find-agency')) {
      $(this).attr("id", "localizador");
    }
  });
}

function getUrlVars() {
  var vars = [],
